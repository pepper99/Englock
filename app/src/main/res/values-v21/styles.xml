<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TestTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@color/primary_dark_material_light</item>
        <item name="android:colorBackground">@color/background_material_light</item>
        <item name="android:navigationBarColor">@color/black</item>
    </style>

    <style name="ThemeGreen" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:colorPrimaryDark">#2baa14</item>
        <item name="android:colorPrimary">#36d519</item>
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/dark_green</item>
        <item name="android:navigationBarColor">?android:attr/colorPrimaryDark</item>
    </style>

    <style name="ThemePink" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:colorPrimaryDark">#cc3166</item>
        <item name="android:colorPrimary">#ff3d80</item>
        <item name="android:colorAccent">#ff3d80</item>
        <item name="colorAccent">@color/accent_pink</item>
        <item name="colorPrimaryDark">@color/pink_dark</item>
        <item name="colorPrimary">@color/accent_pink</item>
        <item name="android:navigationBarColor">?android:attr/colorPrimaryDark</item>
    </style>
</resources>